
#include <cstdlib>
#include<stdio.h>

/*
 * 
 */
int main(int argc, char** argv) {
    int uno,dos;
    int s,r,m,d,md;
    scanf("%d %d",&uno,&dos);
    
    scanf("%d",&s);
    scanf("%d",&r);
    scanf("%d",&m);
    scanf("%d",&d);
    scanf("%d",&md);
    
    if((s==uno+dos)&& (r==uno-dos)&&(m==uno*dos)&&(d==uno/dos)&&(md==uno%dos)){
        printf("\"Correcto\"");
    }
    else
    {
    printf("\"Incorrecto\" \n");
    printf("%d \n",uno+dos);
    printf("%d \n",uno-dos);
    printf("%d \n",uno*dos);
    printf("%d \n",uno/dos);
    printf("%d \n",uno%dos);
    }
    return 0;
}
